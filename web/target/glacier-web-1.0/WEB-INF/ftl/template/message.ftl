<div id="message-{number}" class="message-panel">
    <div class="message-panel-title">
        <div class="message-panel-icon-wrap">
            <div class="btn-icon icon-post"></div>
        </div>
        <div class="message-panel-title-font"></div>
        <div class="message-panel-close" onmousemove="athene.dom.addClass(this,'message-panel-close-hover')"
             onmouseout="athene.dom.removeClass(this,'message-panel-close-hover')">
        </div>
        <div class="clear"></div>
    </div>
    <div class="message-panel-message">
        <div class="message-panel-message-box"></div>
    </div>
</div>